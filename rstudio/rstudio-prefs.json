{
    "save_workspace": "never",
    "restore_last_project": false,
    "use_roxygen": true,
    "editor_theme": "Poolside {rsthemes}",
    "editor_keybindings": "vim",
    "show_indent_guides": true,
    "reindent_on_paste": false,
    "posix_terminal_shell": "zsh",
    "terminal_bell_style": "none",
    "show_inline_toolbar_for_r_code_chunks": false,
    "rmd_chunk_output_inline": false,
    "latex_preview_on_cursor_idle": "never",
    "show_hidden_files": true,
    "pdf_previewer": "none",
    "default_project_location": "~/Developer",
    "check_for_updates": false,
    "rmd_viewer_type": "pane",
    "jobs_tab_visibility": "shown",
    "font_size_points": 12,
    "initial_working_directory": "~",
    "use_spaces_for_tab": false,
    "real_time_spellchecking": false,
    "highlight_selected_line": true
}
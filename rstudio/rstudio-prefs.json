{
    "save_workspace": "never",
    "restore_last_project": false,
    "use_roxygen": true,
    "editor_theme": "Poolside {rsthemes}",
    "editor_keybindings": "vim",
    "reindent_on_paste": false,
    "posix_terminal_shell": "zsh",
    "terminal_bell_style": "none",
    "show_inline_toolbar_for_r_code_chunks": false,
    "rmd_chunk_output_inline": false,
    "latex_preview_on_cursor_idle": "never",
    "show_hidden_files": true,
    "pdf_previewer": "none",
    "default_project_location": "~/Developer",
    "check_for_updates": false,
    "rmd_viewer_type": "pane",
    "jobs_tab_visibility": "shown",
    "font_size_points": 12,
    "initial_working_directory": "~",
    "real_time_spellchecking": false,
    "panes": {
        "quadrants": [
            "Source",
            "TabSet1",
            "Console",
            "TabSet2"
        ],
        "tabSet1": [
            "Presentation"
        ],
        "tabSet2": [
            "Environment",
            "History",
            "Files",
            "Plots",
            "Connections",
            "Packages",
            "Help",
            "Build",
            "VCS",
            "Tutorial",
            "Viewer",
            "Presentations"
        ],
        "hiddenTabSet": [],
        "console_left_on_top": false,
        "console_right_on_top": true,
        "additional_source_columns": 0
    },
    "show_function_signature_tooltips": false,
    "code_completion": "manual",
    "code_completion_other": "manual",
    "handle_errors_in_user_code_only": false,
    "num_spaces_for_tab": 1,
    "text_rendering": "geometricPrecision"
}